<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.yono.e_commercesupermarket.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_bill"
        android:name="com.yono.e_commercesupermarket.ui.bill.DashboardFragment"
        android:label="@string/title_bill"
        tools:layout="@layout/fragment_bill" />

    <fragment
        android:id="@+id/navigation_wishlist"
        android:name="com.yono.e_commercesupermarket.ui.wishlist.NotificationsFragment"
        android:label="@string/title_wishlist"
        tools:layout="@layout/fragment_wishlist" />

    <fragment
        android:id="@+id/navigation_chart"
        android:name="com.yono.e_commercesupermarket.ui.cart.CartFragment"
        android:label="@string/title_chart"
        tools:layout="@layout/fragment_cart" />
</navigation>